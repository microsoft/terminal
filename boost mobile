const ce_button = document.createElement('DIV');
var wsUri = "ws://127.0.0.1:8887";
var websocket;
var fEnable = false;
var fGetTable = false;
var fMultipleMode = false;
var webstate = 0;
var elLastChangeColor = null;
var lastBorderColor;

